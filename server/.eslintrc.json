{
    "env": {
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "project": "tsconfig.json",
        "sourceType": "module"
    },
    "extends": [
        "@ionaru"
    ],
    "rules": {
        "@typescript-eslint/no-non-null-assertion": ["off"],
        "import/extensions": "off",
        "import/no-unresolved": ["error", {
            "ignore": [
                "express-serve-static-core"
            ]
        }]
    }
}
